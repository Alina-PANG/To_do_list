<% if @tasks %>
   <p>No completed tasks yet</p>
<% else %>
<table>
  <h1>Completed Tasks</h1>
  <tr>
    <th>Title</th>
    <th>Importance</th>
    <th>Due Date</th>
  </tr>


  <% @tasks.each do |t| %>
    <tr>
      <td><%= t.title %></td>
      <td><%= t.imp %></td>
      <td><%= t.date %></td>
    </tr>
  <% end %>

</table>
<% end %>
   <button type="button" class="btn btn-default"><%= link_to 'Back', user_list_tasks_path %></button>
